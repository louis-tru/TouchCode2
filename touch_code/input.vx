<?xml version="1.0" encoding="utf-8" ?>
<gui xmlns:view="http://truth-x.org/">
	<head>

		<style>
		  
      .input {
		    border-radius: 5px;
		    border: 1px solid #cccccc;
		    background-color: #fff;
        /*height: 20px;*/
        height: 18px;
        padding: 5px 26px 5px 5px;
        position: relative;
        cursor: text;
        text-align: left;
        font-size: 14px;
        overflow: hidden;
      }
      
      .input input {
        position: relative;
        padding: 0;
        margin: 0;
        appearance: none;
        background: transparent;
        border-width: 0;
        display: block;
        width: 100%;
        height: 100%;
        /*font-size: 16px;*/
        font-size: 14px;
        color: inherit;
        text-align: inherit;
        line-height: initial;
      }
      
      .input .clear {
        background: url(data:image/png;base64,);
        background-position: center center;
        /*background-size: 18px;*/
        /*width: 36px;*/
        background-size: 14px;
        width: 30px;
        height: 100%;
        position: absolute;
        background-repeat: no-repeat;
        top: 0;
        right: -2px;
        cursor: default;
      }
      
      .input #desc_div {
        position: absolute;
        display: block;
        line-height: initial;
        color: #aaa;
        top: 2px;
        bottom: 2px;
        left: 5px;
        right: 26px;
      }
      
      .input #desc_div table {
        position: relative;
        top: 1px;
      }
      
      .input .clear:active{
        opacity: 0.5;
      }
      
		</style>
		
	</head>

  <Input class="input">
    <div id="desc_div">
	    <table border="0" cellpadding="0" cellspacing="0" style="width: 100%; height: 100%"><tr>
	      <td id="desc_text"></td>
	    </tr></table>
    </div>
    <input type="text" id="input" autocorrect="off" autocapitalize="off" _autofocus="autofocus" />
    <div class="clear" onclick="clear" id="clear_btn" />
  </Input>
  
</gui>


